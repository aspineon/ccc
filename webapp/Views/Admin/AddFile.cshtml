@{
    ViewBag.Title = "Dane do raportu";
}

<h2>Dodaj plik</h2>
<h3>Wybierz plik .xls, którego zawartość będzie dołączana do raportu ogólnego</h3>
@using (Html.BeginForm("AddFile", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-md-4">
            <input type="file" name="file"/>
        </div>
        <div class="col-md-8">
            <button type="submit" class="btn btn-default">Wyślij</button>
        </div>
    </div>
}