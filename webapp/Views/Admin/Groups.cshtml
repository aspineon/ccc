@model IEnumerable<webapp.Models.ViewModels.ProducerGroupItem>

@{
    ViewBag.Title = "Producenci - grupy";
}

<h2>Producenci - grupy</h2>

<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Nazwa</th>
                    <th>Grupa</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.Group</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-md-3"></div>
</div>
